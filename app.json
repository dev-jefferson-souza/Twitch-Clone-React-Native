{
  "expo": {
    "name": "twitch-clone",
    "slug": "twitch-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/foregroundImage.png",
      "resizeMode": "contain",
      "backgroundColor": "#9146ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#9146ff"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
